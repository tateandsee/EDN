                      </CardTitle>
                    </CardHeader>
                    <CardContent className="space-y-4">
                      <Button 
                        onClick={() => setShowSocialSchedulerModal(true)}
                        className="w-full"
                        style={{ backgroundColor: colors.primary }}
                      >
                        <Calendar className="mr-2 h-4 w-4" />
