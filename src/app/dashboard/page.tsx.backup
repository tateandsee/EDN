                      </Button>
                      
                      <div className="space-y-3">
                        <h4 className="font-medium" style={{ color: colors.primary }}>Upcoming Posts</h4>
                        {[
                          { platform: 'Instagram', time: '2 hours from now', content: 'New photoshoot 📸' },
                          { platform: 'Twitter', time: '5 hours from now', content: 'Behind the scenes footage' },
                          { platform: 'TikTok', time: 'Tomorrow, 9 AM', content: 'Dance tutorial 🎵' }
                        ].map((post, index) => (
                          <div key={index} className="p-3 rounded-lg bg-white/5">
                            <div className="flex items-center justify-between mb-1">
                              <span className="text-sm font-medium" style={{ color: colors.primary }}>{post.platform}</span>
                              <span className="text-xs" style={{ color: colors.textLight }}>{post.time}</span>
                            </div>
                            <p className="text-xs" style={{ color: colors.textSecondary }}>{post.content}</p>
                          </div>
                        ))}
                      </div>
                      
                      <Button 
                        className="w-full"
                        variant="outline"
                        style={{ borderColor: colors.cardBorder, color: colors.textPrimary }}
                      >
                        <Clock3 className="mr-2 h-4 w-4" />
